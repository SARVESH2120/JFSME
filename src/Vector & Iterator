import java.util.Vector;

public class CollectionDemo {

    public static void main(String[] args) {
        
        Vector<Integer> rollno = new Vector<Integer>();

   int x = 10;
   rollno.add(x);

   rollno.add(45);
   rollno.add(12);
   rollno.add(2, 2000);
   rollno.add(55);
   System.out.println(rollno);

//   rollno.remove(45);  //it takes index

     Integer u = 45;  //it removes value
     rollno.remove(u);

        System.out.println(rollno);


    }
}


