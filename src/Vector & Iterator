-----------------------------------------------------

Collection SORTED
-----------------------------------------------------
import java.util.ArrayList;
import java.util.Collections;
import java.util.Iterator;
import java.util.Vector;

public class CollectionDemo {

    public static void main(String[] args) {

        ArrayList<Integer> rollno = new ArrayList<>();
        int x = 10;
        rollno.add(x);
        rollno.add(45);
        rollno.add(12);
        rollno.add(2, 2000);
        rollno.add(55);


        System.out.println(rollno); //ordered but not sorted

        Collections.sort(rollno);
        System.out.println(rollno);
        System.out.println("desc sort");
        Collections.sort(rollno , Collections.reverseOrder());
        System.out.println(rollno);




    }
}

//Collection - Interface vs Collections -Class
//A comparator is a interface in java that gives a logic for sorting

