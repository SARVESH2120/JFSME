import java.util.ArrayList;
import java.util.Iterator;
import java.util.Vector;

public class CollectionDemo {

    public static void main(String[] args) {

        ArrayList<Integer> rollno = new ArrayList<>();
        int x = 10;
        rollno.add(x);
        rollno.add(45);
        rollno.add(12);
        rollno.add(2, 2000);
        rollno.add(55);

        Iterator<Integer> itr =  rollno.iterator();

        while(itr.hasNext()){
//            System.out.println(itr.next()+1);

//            if(itr.next()%2 == 0)
//                System.out.println("even " + itr.next());
//            else
//                System.out.println("odd " + itr.next());

            int next  = itr.next();
            if(next%2==0){
                System.out.println("even " + next);
            }else{
                System.out.println("odd " + next);
            }
        }




    }
}

