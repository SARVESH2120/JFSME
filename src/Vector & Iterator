import java.util.Vector;

public class CollectionDemo {

    public static void main(String[] args) {

        //Collection works with Objects(Wrapper classes)
//   Vector<Integer> rollno = new Vector<>();
        Vector<Integer> rollno = new Vector<Integer>();   //this is generic type vector not raw type which is previously created

     
        //both are valid syntax

   int x = 10;
   rollno.add(x);  //it is converted behind in string  we call autoboxing
        
   rollno.add(45);
   rollno.add(12);
   rollno.add(2, 2000); //add method is overloaded
//   rollno.add("hello"); m=not applicable after java 1.5


    //here the proof
//        rollno.get(0).   //se after taking index 0 value we got more method after [dot]
    }
}


