import java.util.ArrayList;
import java.util.Iterator;
import java.util.Vector;

public class CollectionDemo {

    public static void main(String[] args) {

        ArrayList<Integer> rollno = new ArrayList<>();
        int x = 10;
        rollno.add(x);
        rollno.add(45);
        rollno.add(12);
        rollno.add(2, 2000);
        rollno.add(55);

        Iterator<Integer> itr =  rollno.iterator();

        System.out.println(rollno);

        while(itr.hasNext()){

            int next  = itr.next();
            System.out.println(next);
            if(next == 45){
                itr.remove(); //45 is removed from collection 
                
            }



        }

        System.out.println(rollno);




    }
}


//iterator[editable] vs enumeration[read only] vs enhanced for loop

