import java.util.HashSet;
import java.util.Set;

public class CollectionDemo {
    
    static Set<Integer> getElements(){
        Set<Integer> mySet = new HashSet<Integer>();
        mySet.add(45);
        mySet.add(43);
        mySet.add(41);
        mySet.add(43);
        mySet.add(44);
        mySet.add(46);
        mySet.add(49);

     return mySet;
    }


    public static void main(String[] args) {
        Set<Integer> elements = getElements();
        System.out.println(elements);
        
    }
}




--------------------------------------------------------------------



import java.util.HashSet;
import java.util.Set;
import java.util.TreeSet;

public class CollectionDemo {

    static Set<Integer> getElements(){
        Set<Integer> mySet = new HashSet<Integer>();
        mySet.add(45);
        mySet.add(43);
        mySet.add(41);
        mySet.add(43);
        mySet.add(44);
        mySet.add(46);
        mySet.add(49);

     return mySet;
    }


    public static void main(String[] args) {
        Set<Integer> elements = getElements();
        System.out.println(elements);


        System.out.println("Elemets after sorted");
        Set<Integer> sortedElements = new TreeSet<>(elements);
        System.out.println(sortedElements);

    }
}



